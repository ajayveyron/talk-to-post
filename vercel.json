{
  "framework": "nextjs",
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Vercel-Skip-Toolbar",
          "value": "1"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/api/auth/twitter/(.*)",
      "has": [
        {
          "type": "host",
          "value": "(?!talk-to-post\\.vercel\\.app).*\\.vercel\\.app"
        }
      ],
      "destination": "https://talk-to-post.vercel.app/api/auth/twitter/$1",
      "permanent": false
    }
  ]
}
